{
  "name": "backend",
  "version": "0.0.1",
  "description": "nestjs application",
  "scripts": {
    "build": "nest build",
    "migration:generate": "dotenvx run -f ../.env -- typeorm-ts-node-commonjs migration:generate -d src/datasource.ts",
    "migration:revert": "dotenvx run -f ../.env -- typeorm-ts-node-commonjs migration:revert -d src/datasource.ts",
    "migration:run": "dotenvx run -f ../.env -- typeorm-ts-node-commonjs migration:run -d src/datasource.ts",
    "eslint": "eslint --fix",
    "node": "node",
    "start": "dotenvx run -f ../.env -- nest start",
    "start:dev": "dotenvx run -f ../.env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenvx run -f ../.env -- node dist/main"
  },
  "dependencies": {
    "@bull-board/api": "^6.11.1",
    "@bull-board/express": "^6.11.1",
    "@bull-board/nestjs": "^6.11.1",
    "@dotenvx/dotenvx": "^1.47.5",
    "@kolya-quizlet/entity": "workspace:*",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.10.0",
    "bullmq": "^5.56.4",
    "express-basic-auth": "^1.2.1",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^5.9.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "reverso-api": "^2.22.10",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.25",
    "typescript": "^5.8.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prisma-json-types-generator": "^3.5.1",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.8.3"
  }
}
